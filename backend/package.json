{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18.x.x",
    "npm": "9.x.x"
  },
  "scripts": {
    "lint:types": "npx tsc --noEmit",
    "lint:ts:generic": "npx eslint --ext .ts",
    "lint:ts": "npm run lint:ts:generic -- .",
    "lint": "npm run lint:types && npm run lint:ts",
    "dev": "nodemon --exec \"node --loader ts-paths-esm-loader\" src/index.ts | npx pino-pretty",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "postbuild": "cp -r package.json build",
    "preview": "node ./build/index.js",
    "test": "vitest"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "pino": "^8.15.0",
    "pino-http": "^8.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/uuid": "^9.0.4",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "ts-paths-esm-loader": "^1.4.3"
  }
}
