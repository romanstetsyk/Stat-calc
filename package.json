{
  "name": "stat-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "check-types": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint \"**/*.{ts,tsx,cjs}\" --config ./.eslintrc.cjs",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{ts,tsx,cjs}\" --fix --config ./.eslintrc.cjs",
    "format": "prettier --check . --config ./.prettierrc.cjs",
    "format:fix": "prettier --write . --config ./.prettierrc.cjs",
    "preview": "vite preview",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --check --config ./.prettierrc.cjs",
      "eslint"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@glideapps/glide-data-grid": "^5.2.1",
    "@react-hook/resize-observer": "^1.2.6",
    "@stdlib/blas-ext-base-gcusum": "^0.0.9",
    "@stdlib/stats-base-dists-normal-cdf": "^0.0.7",
    "@stdlib/stats-base-dists-normal-quantile": "^0.0.7",
    "@stdlib/stats-base-max": "^0.0.9",
    "@stdlib/stats-base-mean": "^0.0.9",
    "@stdlib/stats-base-mediansorted": "^0.0.9",
    "@stdlib/stats-base-min": "^0.0.9",
    "@stdlib/stats-base-range": "^0.0.9",
    "@stdlib/stats-base-stdev": "^0.0.9",
    "@stdlib/stats-base-variance": "^0.0.10",
    "@stdlib/utils-tabulate": "^0.0.8",
    "@stdlib/utils-tabulate-by": "^0.0.8",
    "allotment": "^1.19.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "framer-motion": "^10.12.18",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "marked": "^4.3.0",
    "muuri": "^0.9.5",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.10.1",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.14.1",
    "ruuri": "^2.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}
